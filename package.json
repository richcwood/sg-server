        "@types/stripe": "^8.0.417",
        "bitset": "5.1.1",
        "fast-xml-parser": "^4.2.7",
        "pkg": "5.8.0"
    },
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "jest": "^27.5.1",
        "prettier": "^2.8.3",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0"
    }
