{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Server",
            "processId": "${command:PickProcess}",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/server/dist/server/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "name": "Debug Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server/dist/api/StartServer.js",
            "outFiles": [
                "${workspaceFolder}/server/dist/**/*.js"
            ],
            "env": {
                "KIKI_GIT_USERNAME": "",
                "KIKI_GIT_PASSWORD": "",
                "KIKI_GIT_URL": "",
                "S3_BUCKET_ORG_ARTIFACTS": "",
                "S3_BUCKET_AGENT_BINARIES": "",
                "AWS_SECRET_ACCESS_KEY": "",
                "AWS_ACCESS_KEY_ID": "",
                "AGENT_CREATE_TIMEOUT": "",
                "environment": "debug",
                "NODE_ENV": "debug",
                "SENDGRID_API_KEY": ""
            }
        },
        {
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "name": "Debug Server Workers",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server/dist/RunServerWorkers.js",
            "outFiles": [
                "${workspaceFolder}/server/dist/*.js"
            ],
            "env": {
                "environment": "debug",
                "NODE_ENV": "debug"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "name": "Debug Tests",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/test/dist/test/src/RunUnitTestsHarness.js",
            "args": [
                "Test46.js"
            ],
            "outFiles": [
                "${workspaceFolder}/test/dist/test/**/*.js"
            ],
            "env": {
                "NODE_ENV": "debug"
            }
        }
    ]
}